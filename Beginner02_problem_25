#include <stdio.h>
#include <math.h>

int main() {
    int x_1,y_1,x_2,y_2;
    scanf("%d %d %d %d",&x_1,&y_1,&x_2,&y_2);

    int x_temp = x_1 - x_2;
    int y_temp = y_1 - y_2;

    x_temp *= x_temp;
    y_temp *= y_temp;

    double distance = sqrt(x_temp+y_temp);

    printf("%.2lf\n",distance);

    return 0;
}
